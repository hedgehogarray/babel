<?xml version='1.0' encoding='UTF-8'?>
<package uuid="babel" version="0.0.1">
 <names>
  <name lang="en_US">Babel</name>
 </names>
 <supportedLanguages>
  <language>en_US</language>
 </supportedLanguages>
 <descriptionLanguages>
  <language>en_US</language>
 </descriptionLanguages>
 <contents>
  <behaviorContent path=".">
   <userRequestable/>
   <nature>interactive</nature>
   <triggerSentences>
    <sentence lang="en_US">translate</sentence>
    <sentence lang="en_US">translator</sentence>
   </triggerSentences>
   <permissions/>
  </behaviorContent>
 </contents>
 <requirements>
  <naoqiRequirement minVersion="2.3"/>
  <robotRequirement/>
 </requirements>
 <services>
  <service autorun="false" execStart="/usr/bin/python2 scripts/main.py --qi-url [URL]" name="main"/>
 </services>
</package>
